            },
            "event_handlerssssss": {
                "document_changed": [
                    {"handler": "webhook",
                     "url": "http://localhost:8081",
                     "filter": `function(doc) {
                        if (doc.class == "English" && doc.grade == "A" && doc.count > 3 && doc.bool == true) {
                            return true;
                        }
                        return false;
                     }`
                    } 
                ]
            }
        }    
    }

query:
    SELECT select_clause from BUCKET_NAME WHERE where_clause GROUP BY group_by_clause HAVING having_clause ORDER BY order_by_clause ;

select_clause:
    NUMERIC_FIELD, BOOL_FIELD | NUMERIC_FIELD, BOOL_ALIAS | NUMERIC_FIELD, BOOL_FIELD BOOL_ALIAS |
    NUMERIC_ALIAS, BOOL_FIELD | NUMERIC_ALIAS, BOOL_ALIAS | NUMERIC_ALIAS, BOOL_FIELD BOOL_ALIAS |
    NUMERIC_FIELD NUMERIC_ALIAS, BOOL_FIELD | NUMERIC_FIELD NUMERIC_ALIAS, BOOL_ALIAS | NUMERIC_FIELD NUMERIC_ALIAS, BOOL_FIELD BOOL_ALIAS ;

where_clause:
    NUMERIC_FIELD > 10 and_or BOOL_FIELD = true | NUMERIC_FIELD > 10 | BOOL_FIELD = true;

and_or:
    AND | OR ;

group_by_clause:
    NUMERIC_FIELD, BOOL_FIELD  | NUMERIC_FIELD, BOOL_FIELD BOOL_ALIAS |
    NUMERIC_FIELD NUMERIC_ALIAS, BOOL_FIELD |  NUMERIC_FIELD NUMERIC_ALIAS, BOOL_FIELD BOOL_ALIAS ;

having_clause:
    (NUMERIC_FIELD > 10 and_or BOOL_FIELD = true) | (NUMERIC_FIELD > 10 and_or BOOL_ALIAS = true) |
    (NUMERIC_ALIAS > 10 and_or BOOL_FIELD = true) | (NUMERIC_ALIAS > 10 and_or BOOL_ALIAS = true) ;

order_by_clause:
    NUMERIC_FIELD, BOOL_FIELD | NUMERIC_FIELD, BOOL_ALIAS |
    NUMERIC_ALIAS, BOOL_FIELD | NUMERIC_ALIAS, BOOL_ALIAS ;






